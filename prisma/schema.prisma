generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Group {
  id    Int     @id @default(autoincrement())
  group_id Int
  domain String
  post_id Int
  photo Photo[]
}

model Photo {
  id    Int     @id @default(autoincrement())
  photo_id Int
  name String
  
  group_id Int
  group Group @relation(fields: [group_id], references: [id])
}