generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Group {
  id    Int     @id @default(autoincrement())
  group_id Int @unique
  domain String @unique
  post_id Int
  image Image[]
}

model Image {
  id    Int     @id @default(autoincrement())
  group_id Int
  group Group @relation(fields: [group_id], references: [id])
}